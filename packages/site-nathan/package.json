{
  "name": "@ndemasie/site",
  "description": "Personal website of Nathan DeMasie",
  "version": "0.4.0",
  "author": "Nathan DeMasie (https://nathan.demasie.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ndemasie/ndemasie.github.io/issues"
  },
  "homepage": "https://nathan.demasie.com",
  "scripts": {
    "astro:build": "npx astro build",
    "astro:dev": "npx astro dev",
    "dev": "npx astro dev",
    "format:check": "npx prettier --check \"src/**/*.{astro,js,jsx,ts,tsx,css,scss,sass,md}\"",
    "format:write": "npx prettier --write \"src/**/*.{astro,js,jsx,ts,tsx,css,scss,sass,md}\"",
    "lint:check": "npx eslint src",
    "lint:fix": "npx eslint src --fix",
    "update:pkgs": "npx npm-check --update",
    "update:lock": "npm i --package-lock-only --workspaces false",
    "watch:public": "npx nodemon --ext 'tsx' --watch './src/**/public/**/*.tsx' --exec 'bash ./scripts/copy_public_dirs.sh'"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "11.1.0",
    "@codemirror/lang-javascript": "6.2.1",
    "@emotion/css": "11.10.6",
    "@emotion/react": "11.10.6",
    "@uiw/codemirror-theme-vscode": "4.23.7",
    "@uiw/react-codemirror": "4.23.7",
    "@webcomponents/template-shadowroot": "0.2.1",
    "@webcontainer/api": "1.5.0",
    "d3": "7.9.0",
    "i18next": "23.6.0",
    "lit": "2.7.6",
    "lodash": "4.17.21",
    "modern-normalize": "2.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "13.3.1",
    "react-router-dom": "6.17.0",
    "react-use": "17.4.0",
    "react-use-websocket": "4.5.0",
    "xterm": "5.3.0",
    "xterm-addon-fit": "0.8.0"
  },
  "devDependencies": {
    "@astrojs/lit": "^3.0.1",
    "@astrojs/node": "^6.0.3",
    "@astrojs/react": "^3.0.4",
    "@astrojs/sitemap": "^3.0.2",
    "@astrojs/svelte": "^4.0.3",
    "@astrojs/ts-plugin": "^1.0.7",
    "@types/d3": "^7.4.2",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "astro": "^3.4.0",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "node-html-parser": "^6.1.11",
    "postcss-logical": "^7.0.0",
    "postcss-preset-env": "^9.2.0",
    "sass": "^1.69.5",
    "ts-node": "^10.9.1"
  }
}